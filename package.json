{
  "name": "react-native-typescript-starter",
  "description": "React Native starter application, TypeScript with React Native Navigation",
  "keywords": [
    "react native",
    "typescript",
    "boilerplate",
    "react native navigation"
  ],
  "version": "v0.0.1",
  "author": {
    "name": "Lucas 'Lucactusss' Martin-Chabalier",
    "email": "lucas.martinchabalier@gmail.com",
    "url": "https://github.com/lucactusss"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git://github.com/lucactusss/react-native-typescript-starter"
  },
  "bugs": {
    "url": "https://github.com/lucactusss/react-native-typescript-starter/issues",
    "email": "lucas.martinchabalier@gmail.com"
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "cross-env-shell jest",
    "android": "react-native run-android --variant=Debug",
    "lint": "cross-env-shell tslint --project ./tsconfig.json",
    "lint:fix": "cross-env-shell tslint --fix --project ./tsconfig.json"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-native": "^0.60.5",
    "react-native-navigation": "^6.7.1",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.2",
    "@types/react-native": "^0.63.6",
    "@types/react-redux": "^7.1.2",
    "@types/react-test-renderer": "^16.9.0",
    "@types/redux-logger": "^3.0.7",
    "@babel/core": "^7.6.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "24.9.0",
    "babel-preset-airbnb": "^5.0.0",
    "babel-preset-react-native": "^4.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-watch": "^6.0.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-addons-test-utils": "^15.6.2",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "16.9.0",
    "regenerator-runtime": "^0.13.3",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.6.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!ios/**",
      "!android/**"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.9.2"
  }
}